          <maml:description>
            <maml:para>Enter the first month to display by date, like 1/1/2019.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
